{
   "manifest_version": 3,
   "name": "TimeTracker",
   "description": "Track your web and app browsing habits!",
   "version": "1.0",
   "author": "Tarun Khare",
   "icons": { "48": "icon.png" },
   "action": {
      "default_icon": "icon.PNG",
      "default_title": "Just observing your current url.",
      "default_popup": "popup.html"
   },
   "permissions": ["tabs", "activeTab", "storage"],
   "host_permissions": ["https://localhost:5000/"],
   "background": {
      "service_worker": "background.js"
   }
}
